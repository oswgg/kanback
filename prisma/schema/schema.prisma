datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["prismaSchemaFolder"]
}

model Organization {
    uuid         String   @id @default(uuid())
    name         String   @db.VarChar(100)
    image_url    String?
    welcome_text String?  @db.MediumText
    created_at   DateTime @default(now())
    users        User[]
}
